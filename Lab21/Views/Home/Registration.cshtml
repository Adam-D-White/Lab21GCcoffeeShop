
@{
    ViewBag.Title = "Registration";
}

<h2>Registration</h2>

<form action="/Home/NewUser" method="get">

    <div class="inputLook">
        
            First Name: <input type="text" name="FirstName" placeholder="Enter First Name" required /><br />
            Last Name: <input type="text" name="LastName" placeholder="Enter Last Name" required /><br />
            Username: <input type="text" name="UserName" placeholder="Enter Username" required /><br />
            Email: <input type="email" name="Email" placeholder="Enter Email" required /><br />
            Password: <input type="password" name="Password" placeholder="Enter Password" required /><br />
        
    </div>

    
    
    
    
    


    <input type="submit" value="Submit Info"/><a href="/Home/User"></a> 
    




</form>